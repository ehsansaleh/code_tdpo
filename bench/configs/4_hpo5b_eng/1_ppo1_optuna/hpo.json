{
  "library": "optuna",
  "param_props": {
    "gamma": {"range":[0.48800, 0.99996875], "type":"float", "do_log":true,
      "transform":{"forward": "1.0/(1.0-x)", "backward": "1.0-(1.0/y)"}},
    "lam": {"range":[0.48800, 0.99993750], "type":"float", "do_log":true,
      "transform":{"forward": "1.0/(1.0-x)", "backward": "1.0-(1.0/y)"}},
    "timesteps_per_actorbatch": {"range":[117, 960000], "type":"int", "do_log":true, "transform":null},
    "clip_param": {"range":[0.02, 200], "type":"float", "do_log":true, "transform":null},
    "entcoeff": {"range":[0.0, 0.001], "type":"float", "do_log":true,
      "transform":{"forward": "x+0.00001", "backward": "y-0.00001"}},
    "optim_epochs": {"range":[1, 128], "type":"int", "do_log":true, "transform":null},
    "optim_stepsize": {"range":[0.00001, 0.1], "type":"float", "do_log":true, "transform":null},
    "optim_minibatches": {"range":[1, 128], "type":"int", "do_log":true, "transform":null},
    "adam_epsilon": {"range":[1e-8, 1e-4], "type":"float", "do_log":true, "transform":null},
    "schedule": {"choices":["linear", "constant"], "type":"cat"}
  },
  "hpo_superparams": {"sampler": "TPE", "seed": 12345, "batch_size": 10, "num_iters": 25},
  "hpo_iternames": "round_*",
  "init_experiment": "ovat5b",
  "hpo_experiment": "hpo5b",
  "run2perf": [["max", "traj_return"], "traj_return"],
  "rngseed_reduction": "mean",
  "pick_random_rngseeds": false,
  "template_cfg": {
    "description": "Hyper-parameter optimization for ppo1 on a low frequency environment",
    "method": "ppo1",
    "environment": "drop_leg_4k",
    "total_timesteps" : 5000000000,
    "rng_seed" : [0, 1000, 2000],
    "policy_kwargs": {"net_arch" : [{"vf": [64, 64], "pi": [64, 64]}]},
    "gamma": 0.99975,
    "lam": 0.99950,
    "timesteps_per_actorbatch": 240000,
    "clip_param": 0.2,
    "entcoeff": 0.0,
    "optim_epochs": 4,
    "optim_stepsize": 0.001,
    "optim_minibatches": 4,
    "adam_epsilon": 1e-5,
    "schedule": "linear",
    "num_envs": 24,
    "results_dir_name": "results",
    "storage_dir_name": "storage",
    "train_set": "remaining",
    "eval_set": "remaining",
    "OPENAI_LOG_FORMAT": "csv",
    "walltime_hrs": 10,
    "num_checkpoints": 1000,
    "eval_ntrajs":10, "eval_nsteps":8000, "eval_seed": 12345,
    "looping_tree": {
      "method": "fixed",
      "environment": "fixed",
      "total_timesteps" : "fixed",
      "rng_seed": "cartesian",
      "policy_kwargs": "fixed",
      "gamma": "fixed",
      "lam": "fixed",
      "timesteps_per_actorbatch": "fixed",
      "clip_param": "fixed",
      "entcoeff": "fixed",
      "optim_epochs": "fixed",
      "optim_stepsize": "fixed",
      "optim_minibatches": "fixed",
      "adam_epsilon": "fixed",
      "schedule": "fixed"
    }
  }
}
